          <cols>
            <col id="1">
                <field>010</field>
                <subfield>a</subfield>  <!-- ISBN -->
            </col>
            <col id="2,3,4">
                <field>995</field>
                <subfield>f,k,r</subfield> <!-- Código de barras ejemplar, signatura, tipo de documento -->
        </col>
        <col id="5,6,7,8">
                <field>200</field>
                <subfield>a,c,d,e</subfield> <!-- Titulo del libro, Título propio de un autor diferente, Título paralelo, Complemento -->
            </col>
            <col id="9">
                <field>700</field>
                <subfield>a</subfield> <!-- Datos Autor_1 -->
            </col>
            <col id="10">
                <field>701</field>
                <subfield>a</subfield> <!-- Datos Autor_2 -->
            </col>
            <col id="11">
                <field>702</field>
                <subfield>a</subfield> <!-- Datos Autor_3 -->
            </col>
            <col id="12,13">
                <field>210</field>
                <subfield>c,d</subfield> <!-- Editorial y Año publicación libro -->
            </col>
            <col id="14">
                <field>225</field>
                <subfield>a</subfield> <!-- Colección. Ejem. barco de vapor etc... -->
            </col>
            <col id="15,16,17">
                <field>215</field>
                <subfield>a,c,d</subfield> <!-- Número de páginas, Características del libro, Formato del libro -->
            </col>
            <col id="18">
                <field>300</field>
                <subfield>a</subfield> <!-- Notas generales -->
            </col>           
            <col id="19">
                <field>676</field>
                <subfield>a</subfield> <!-- Clasificación -->
            </col>      
            <col id="20,21">
                <field>101</field>
                <subfield>a,c</subfield> <!-- Idioma de la publicación e Idioma general -->
            </col>
    </cols>
    </step>
</params>

